{
    "name": "@slimio/jsdoc",
    "version": "0.1.2",
    "description": "JSDoc Generator for SlimIO Projects",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "prepublishOnly": "pkg-ok",
        "test": "ava --verbose",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "c8 -r=\"html\" npm test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/jsdoc.git"
    },
    "keywords": [
        "jsdoc",
        "parser",
        "linker",
        "generator"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/jsdoc/issues"
    },
    "homepage": "https://github.com/SlimIO/jsdoc#readme",
    "dependencies": {
        "jsdoc-extractor": "^3.0.0",
        "jsdoc-tokenizer": "^1.3.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^7.6.1",
        "@commitlint/config-conventional": "^7.6.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^2.0.4",
        "ava": "^1.4.1",
        "dotenv": "^8.0.0",
        "eslint": "^5.16.0",
        "husky": "^1.3.1",
        "jsdoc": "^3.5.5",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=10"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
